{
  "name": "lirest_fe",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@7.0.0",
  "description": "Lirest UI",
  "license": "MIT",
  "author": "Tran Lam",
  "main": "src/main.ts",
  "scripts": {
    "prepare": "husky install && husky add .husky/pre-commit \"lint-staged\"",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "build": "vite build",
    "build:clean": "rimraf dist",
    "dev": "vite --port 3333 --open",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "3.0.0-5",
    "@vee-validate/rules": "^4.5.11",
    "@vueuse/core": "^8.4.2",
    "@vueuse/head": "^0.7.6",
    "axios": "^0.27.2",
    "jwt-decode": "^3.1.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "prism-theme-vars": "^0.2.2",
    "vee-validate": "^4.5.11",
    "vue": "^3.2.33",
    "vue-axios": "^3.4.1",
    "vue-demi": "^0.12.5",
    "vue-i18n": "9.1.9",
    "vue-router": "^4.0.15"
  },
  "optionalDependencies": {
    "cypress": "^9.6.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.0",
    "@iconify-json/carbon": "^1.1.4",
    "@iconify/json": "^2.1.57",
    "@intlify/vite-plugin-vue-i18n": "3.4.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^2.3.2",
    "@vue/test-utils": "^2.0.0-rc.21",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.4",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.52.2",
    "typescript": "^4.6.4",
    "unocss": "^0.33.2",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.8",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-ssg": "^0.20.0",
    "vite-ssg-sitemap": "^0.2.6",
    "vitest": "^0.12.4",
    "vue-tsc": "^0.34.12"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --cache --fix"
  }
}
